<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.0.12+</version>
	</compatibility>
	<theme id="innovationsslussen-theme" name="Innovationsslussen Theme">
		<settings>
			<setting key="show-breadcrumbs" value="true" />
		</settings>
	</theme>
	<theme id="innovationsslussen-no-breadcrumbs" name="Innovationsslussen Theme (no breadcrumbs)">
		<settings>
			<setting key="show-breadcrumbs" value="false" />
		</settings>
	</theme>
	
</look-and-feel>